<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="login-action" namespace="/sys" extends="base-default">
		<action name="login_*" class="com.money.login.action.LoginAction" method="{1}">
			<result name="{1}">/WEB-INF/jsp/xfgl/user/{1}.jsp</result>
			<result name="loginUI">/WEB-INF/jsp/loginUI.jsp</result>
		</action>
	</package>

</struts>
